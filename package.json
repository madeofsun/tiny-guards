{
  "name": "tiny-guards",
  "version": "2.0.3",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --noEmit false",
    "eslint": "eslint . --ext .js,.ts",
    "typescript": "tsc && tsc -p tests/tsconfig.json && tsc -p scripts/jsconfig.json && tsc -p jest-config/jsconfig.json",
    "jest": "jest --no-cache",
    "jest:coverage": "npm run jest -- --coverage",
    "jest:built": "DIST=dist npm run jest",
    "test-pkg": "cd test-pkg && npm install && npm run test:ts && npm run test:js",
    "gen:index": "node scripts/gen-index.js",
    "gen:test-pkg": "node scripts/gen-test-pkg.js",
    "change:add": "npx changeset add"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@changesets/changelog-github": "0.5.0",
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-typescript": "7.23.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@types/jest": "29.5.12",
    "@types/node": "^20.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "lodash-es": "4.17.21",
    "prettier": "3.2.5",
    "typescript": "5.5.2"
  },
  "keywords": [
    "typescript",
    "guards",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeofsun/tiny-guards"
  },
  "author": "madeofsun (https://github.com/madeofsun)",
  "license": "MIT"
}
