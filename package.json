{
  "name": "tiny-guards",
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "eslint . --ext .js,.ts",
    "test": "jest",
    "test:coverage": "npm run test -- --collect-coverage",
    "test:build": "npm run build && USE_DIST=js npm run test && USE_DIST=cjs npm run test",
    "publish": "npm run build && cd dist && npm publish",
    "prepack": "echo '\nUse `npm run publish`\n' && exit 1"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@types/jest": "29.5.1",
    "@types/node": "^16.0.0",
    "chokidar": "3.5.3",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "typescript": "5.1.3",
    "ts-node": "10.9.1"
  },
  "keywords": [
    "typescript",
    "guards",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeofsun/tiny-guards"
  },
  "author": "madeofsun (https://github.com/madeofsun)",
  "license": "MIT"
}
