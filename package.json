{
  "name": "tiny-guards",
  "version": "3.0.0-alpha.0",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint . --ext .js,.ts",
    "types": "tsc && tsc -p tests/tsconfig.json && tsc -p scripts/jsconfig.json && tsc -p jest-config/jsconfig.json",
    "test": "jest --no-cache",
    "test:coverage": "npm run test -- --collect-coverage",
    "test:build": "DIST=dist npm run test",
    "test:pkg": "cd test-pkg && npm install && npm run test:types && npm run test:run",
    "change:add": "npx changeset add"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@changesets/changelog-github": "0.5.0",
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-typescript": "7.23.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@types/jest": "29.5.12",
    "@types/node": "^20.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "lodash-es": "4.17.21",
    "prettier": "3.2.5",
    "typescript": "5.5.2"
  },
  "keywords": [
    "typescript",
    "guards",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeofsun/tiny-guards"
  },
  "author": "madeofsun (https://github.com/madeofsun)",
  "license": "MIT"
}
